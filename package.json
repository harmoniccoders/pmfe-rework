{
  "name": "nextarter-chakra",
  "version": "0.1.0",
  "private": true,
  "author": "sozonome",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "postbuild": "next-sitemap --config next-sitemap.config.js",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "eslint src --fix && yarn format",
    "type-check": "tsc --noEmit",
    "format": "prettier --write src",
    "up": "yarn upgrade-interactive",
    "up-latest": "yarn up --latest",
    "release": "standard-version",
    "push-release": "git push --follow-tags origin main",
    "prepare": "husky install",
    "schema:fetch": "node ./script/fetch-schema.js",
    "schema:hooks": "openapi --input ./src/types/api-schema.json --output ./src/types",
    "schema:generate": "node ./script/schema-codegen.js --srcPath ./src/types/api-schema.json --destPath ./src/types/api --separatedFiles true",
    "schema:codegen": "yarn schema:fetch && yarn schema:generate",
    "format-staged": "pretty-quick --staged"
  },
  "dependencies": {
    "@babel/preset-env": "^7.16.11",
    "@chakra-ui/core": "0.8.0",
    "@chakra-ui/react": "^1.8.8",
    "@codegena/oapi3ts-cli": "^2.1.10-alpha.8",
    "@emotion/react": "^11",
    "@emotion/styled": "^11",
    "@hookform/resolvers": "^2.8.8",
    "@types/file-saver": "^2.0.5",
    "@types/js-cookie": "^3.0.2",
    "@uploadcare/react-widget": "^2.0.0",
    "animate.css": "^4.1.1",
    "axios": "^0.26.1",
    "chart.js": "^3.7.1",
    "eslint-import-resolver-typescript": "^2.7.1",
    "file-saver": "^2.0.5",
    "framer-motion": "^6",
    "js-cookie": "^3.0.1",
    "moment": "^2.29.3",
    "next": "^12.1.0",
    "next-pwa": "^5.4.6",
    "next-seo": "^5.3.0",
    "openapi-typescript-codegen": "^0.21.0",
    "postmark": "^3.0.1",
    "react": "^17.0.2",
    "react-chartjs-2": "^4.1.0",
    "react-dom": "^17.0.2",
    "react-hook-form": "^7.30.0",
    "react-icons": "^4.3.1",
    "react-loader-spinner": "^6.0.0-0",
    "react-multi-carousel": "^2.8.0",
    "react-openapi-client": "^0.2.2",
    "react-reveal": "^1.2.2",
    "react-smarttab": "^2.0.1",
    "react-toast-notifications": "^2.5.1",
    "react-use-window-scroll": "^1.0.13",
    "typescript-eslint-parser": "^22.0.0",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/eslint-parser": "7.16.3",
    "@babel/preset-react": "^7.16.7",
    "@commitlint/config-conventional": "^16.2.1",
    "@types/node": "16.11.12",
    "@types/react": "^17.0.40",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "^5.19.0",
    "commitlint": "^16.2.3",
    "eslint": "8.4.1",
    "eslint-config-airbnb": "19.0.2",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-next": "^12.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-config-sznm": "^0.2.1",
    "eslint-config-wesbos": "3.0.2",
    "eslint-plugin-html": "6.2.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.27.1",
    "eslint-plugin-react-hooks": "4.3.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.3.7",
    "next-sitemap": "^2.5.1",
    "prettier": "2.5.1",
    "standard-version": "^9.3.2",
    "typescript": "^4.6.3"
  }
}
